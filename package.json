{
  "name": "react-native-dotnet-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "webpack --config ./web/webpack.config.js --env NODE_ENV=dev",
    "web:prod": "webpack --config ./web/webpack.config.js --env NODE_ENV=prod",
    "web:serve": "npm run web -- --env NODE_DEV_SERVER=true && webpack serve --config ./web/webpack.config.js --env NODE_ENV=dev --env NODE_DEV_SERVER=true",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate-client": "node ./client-build/nswag.js"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.12.2",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.1",
    "babel-loader": "^8.2.2",
    "css-mediaquery": "^0.1.2",
    "react": "16.13.1",
    "react-dom": "^17.0.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.2",
    "react-native-web": "^0.14.10",
    "react-responsive": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.8.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "^0.14.10",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.5.1",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "nswag": "^13.10.1",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "^3.0.2",
    "swagger-parser": "^10.0.2",
    "tmp": "^0.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
